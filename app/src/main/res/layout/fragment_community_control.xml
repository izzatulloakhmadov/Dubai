<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CommunityControlFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imagev"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_margin="24dp"
            android:background="@drawable/dumo"
            android:padding="17dp"
            android:src="@drawable/ic_iconss"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/add_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="162dp"
            android:layout_marginTop="38dp"
            android:layout_marginEnd="161dp"
            android:text="Community Control"
            android:textColor="#252B5C"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:id="@+id/liner"
            app:layout_constraintTop_toBottomOf="@id/add_user"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="327dp"
            android:layout_height="225dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"

            android:layout_marginEnd="24dp"
            android:background="@drawable/rec">

            <ImageButton
                android:layout_width="311dp"
                android:layout_height="175dp"
                android:layout_margin="8dp"
                android:background="@drawable/rad"
                android:clipToOutline="true"
                android:scaleType="centerCrop"
                android:src="@drawable/n" />


            <TextView
                android:id="@+id/textview"
                android:layout_width="90dp"
                android:layout_height="18dp"
                android:layout_marginStart="-310dp"
                android:layout_marginTop="190dp"
                android:text="Dubai hils"
                android:textColor="#252B5C"
                android:textSize="16sp" />


        </LinearLayout>
        <LinearLayout
            android:id="@+id/liner1"
            app:layout_constraintTop_toBottomOf="@id/liner"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="327dp"
            android:layout_height="225dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"

            android:layout_marginEnd="24dp"
            android:background="@drawable/rec">

            <ImageButton
                android:layout_width="311dp"
                android:layout_height="175dp"
                android:layout_margin="8dp"
                android:background="@drawable/rad"
                android:clipToOutline="true"
                android:scaleType="centerCrop"
                android:src="@drawable/n" />


            <TextView
                android:id="@+id/textview1"
                android:layout_width="90dp"
                android:layout_height="18dp"
                android:layout_marginStart="-310dp"
                android:layout_marginTop="190dp"
                android:text="Palm hili"
                android:textColor="#252B5C"
                android:textSize="16sp" />


        </LinearLayout>


        <ImageView
            android:id="@+id/imagex"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="600dp"
            android:layout_marginEnd="24dp"
            android:background="@drawable/dumoq"
            android:padding="20dp"
            android:src="@drawable/ic_baseline_add_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.core.widget.NestedScrollView>